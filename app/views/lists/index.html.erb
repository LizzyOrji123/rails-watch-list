<h1>List of Movies</h1>
<div class="lists">
  <div class="row justify-content-center">
    <div class="col-12 col-md-8">
        <h1>Add new list for <%= @movies.list %></h1>
        <% @lists.each do |list| %>
            <%= render list %>
            <<%= f.input :list, collection: List.all %>
            <%= f.submit "Add list", %>
              <%= link_to "Show this list", list %>
        <% end %>
    </div>
  </div>
</div>
